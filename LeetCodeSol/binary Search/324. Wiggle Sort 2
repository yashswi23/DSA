class Solution {
    public void wiggleSort(int[] nums) {
        int n  = nums.length;
        int[] sorte = nums.clone();
        Arrays.sort(sorte);

        int mid =(n+1)/2;
        int j = mid-1;
        int k = n-1;

        for(int i=0;i<n;i++){
            if(i%2==0){
                nums[i] = sorte[j--];
            }
            else{
                nums[i] = sorte[k--];
            }
        }
    }
}
