class Solution {
    public boolean isIsomorphic(String s, String t) {
        int[] maps = new int[256];
        int []mapt = new int[256];

        for(int i=0;i<s.length();i++){
            if(maps[s.charAt(i)]!= mapt[t.charAt(i)]){
                return false;
            }
            maps[s.charAt(i)]=i+1;
            mapt[t.charAt(i)]=i+1;
        }
        return true;
    }
}
