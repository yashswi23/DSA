class Solution {
    public int maxProfit(int[] prices) {
        int n =prices.length;
        int maxpro=0;
        int min= prices[0];
        for(int i=1;i<n;i++){
            maxpro = Math.max(maxpro, prices[i]-min);
             min = Math.min(min,prices[i]);
        }

        return maxpro;
    }
}
