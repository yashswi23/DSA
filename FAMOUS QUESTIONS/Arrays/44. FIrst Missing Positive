class Solution {
    public int firstMissingPositive(int[] nums) {
        int n = nums.length;
        Map<Integer,Boolean> mp = new HashMap<>();
        for(int num: nums){
            if(num>0){
                mp.put(num,true);
            }
        }
       
       for(int i=1;i<=n+1;i++){
        if(!mp.containsKey(i)){
            return i;
        }
       }
       return 1;
    }
}
