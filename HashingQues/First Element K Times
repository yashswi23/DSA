class Solution {
    public int firstElementKTime(int[] arr, int k) {
        // write code
        
        Map<Integer,Integer> mp = new HashMap<>();
        int n = arr.length;
        for(int i=0;i<n;i++){
            int num = arr[i];
            
            mp.put(num,mp.getOrDefault(num,0)+1);
            
            if(mp.get(num)>=k){
                return num;
            }
        }
        return -1;
    }
}
