class Solution {
    public int countSubstring(String s) {
        // code here
        int n = s.length();
        
        int[] freq = new int[26];
        for(int i=0;i<n;i++){
            freq[s.charAt(i)-'a']++;
        }
        
        int res=0;
        
        for(int f: freq){
            res+= (f*(f+1))/2;
        }
        
        return res;
        
    }
}
