class Solution {
    public int equalPairs(int[][] grid) {
        int n = grid.length;
        int count=0;

        for(int row=0;row<n;row++){
            for(int col=0;col<n;col++){
                boolean isequal = true;
                for(int i=0;i<n;i++){
                    if(grid[row][i]!= grid[i][col]){
                        isequal= false;
                        break;
                    }
                }
                if(isequal == true){
                    count++;
                }
            }

        }
        return count;
    }
}
